app:
    logger:
        - console
        - file

services:
    ip:
        service: https://api.ipify.org?format=json
        interval: 10000

    cloudflare:
        account:
        api_key:
        zone_id:

    logger:
        console:
            colorize: true
            timestamp: true

        file:
            filename: "log/log-"
            datePattern: "yyyy-MM-dd.log"
